      } ]},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$ResourceInArea", "args": [ {
        "enum": "games.dicemonastery.DiceMonasteryConstants$Resource",
        "value": "PALE_BLUE_PIGMENT"
      }, {
        "enum": "games.dicemonastery.DiceMonasteryConstants$ActionArea",
        "value": "STOREROOM"
      } ]},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$MonkPiety", "args": [ "4" ]}

{
  "class": "evaluation.listeners.GameListener",
  "args": [
    {
      "class" : "evaluation.loggers.SummaryLogger"
    },
    [
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Season"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Year"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Piety"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Location"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Thing"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Value"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$ActionPointsLeft"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Turn"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$WritingSets"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Pigments"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$Inks"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$MonksCount"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$PietySum"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$VictoryPoints"},
      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$TreasureSum"},

      {"class": "games.dicemonastery.stats.DiceMonasteryMetrics$IsSeason", "args": [ {
        "enum": "games.dicemonastery.DiceMonasteryConstants$Season",

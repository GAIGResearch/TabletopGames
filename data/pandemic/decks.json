     "name": ["String","Events"],
     "visibility" : "HIDDEN_TO_ALL",
     "cards":
      [
       {
       "name": ["String","Resilient Population"],
       "effect": ["String","Remove any 1 card in the Infection Discard Pile from the game. You may play this between the Infect and Intensify steps of an epidemic."]
       },
       {
         "name": ["String","Airlift"],
         "effect": ["String","Move any 1 pawn to any city. Get permission before moving another player's pawn."]
       },
       {
         "name": ["String","Government Grant"],
         "effect": ["String","Add 1 research station to any city (no City card needed)."]
       },
       {
         "name": ["String","One quiet night"],
         "effect": ["String","Skip the next Infect Cities step (do not flip over any Infection cards)."]
       },
       {
         "name": ["String","Forecast"],
         "effect": ["String","Draw, look at, and rearrange the top 6 cards of the Infection Deck. Put them back on top."]
       }
      ]
